{
  "name": "bar",
  "layer": "bottom",
  "position": "bottom",
  "height": 12,
  "spacing": 4,
  "modules-left": [
    "custom/logout_menu",
    "bluetooth",
    "network",
    "cpu",
    "memory",
    "temperature",
    "pulseaudio",
    "systemd-failed-units",
    "group/misc"
  ],
  "modules-center": [
    "hyprland/workspaces",
    "hyprland/submap"
  ],
  "modules-right": [
    "clock#time",
    "custom/separator",
    "clock#calendar"
  ],
  "hyprland/workspaces": {
    "on-click": "activate",
    "format": "{icon}",
    "format-icons": {
      "1": "Û∞≤†",
      "2": "Û∞≤¢",
      "3": "Û∞≤§",
      "4": "Û∞≤¶",
      "5": "Û∞≤®",
      "6": "Û∞≤™",
      "7": "Û∞≤¨",
      "8": "Û∞≤Æ",
      "9": "Û∞≤∞",
      "10": "Û∞ø¨",
      "special": "ÔÜí"
      // "active": "ÔÜí",
      // "default": "ÔÑë",
      // "empty": "ÔÑå"
    },
    "show-special": true,
    "persistent-workspaces": {
      "1": [],
      "2": [],
      "3": [],
      "4": [],
      "5": [],
      "6": [],
      "7": [],
      "8": [],
      "9": []
    }
  },
  "hyprland/submap": {
    "format": "<span color='#a6da95'>Mode:</span> {}",
    "tooltip": false
  },
  "clock#time": {
    "format": "{:%I:%M %p}",
    "locale": "en_US.UTF-8",
    "timezones": [
      "America/Chicago"
    ]
  },
  "custom/separator": {
    "format": "|",
    "tooltip": false
  },
  "custom/separator_dot": {
    "format": "‚Ä¢",
    "tooltip": false
  },
  "clock#week": {
    "format": "{:%a}"
  },
  "clock#month": {
    "format": "{:%h}"
  },
  "clock#calendar": {
    "format": "{:%m/%d}",
    "tooltip-format": "<tt><small>{calendar}</small></tt>",
    "actions": {
      "on-click-right": "mode"
    },
    "calendar": {
      "mode": "month",
      "mode-mon-col": 3,
      "weeks-pos": "right",
      "on-scroll": 1,
      "on-click-right": "mode",
      "format": {
        "months": "<span color='#f4dbd6'><b>{}</b></span>",
        "days": "<span color='#cad3f5'><b>{}</b></span>",
        "weeks": "<span color='#c6a0f6'><b>W{}</b></span>",
        "weekdays": "<span color='#a6da95'><b>{}</b></span>",
        "today": "<span color='#8bd5ca'><b><u>{}</u></b></span>"
      }
    }
  },
  "clock": {
    "format": "{:%I:%M %p %Ez | %a ‚Ä¢ %h | %F}",
    "format-alt": "{:%I:%M %p}",
    "tooltip-format": "<tt><small>{calendar}</small></tt>",
    // "locale": "en_US.UTF-8",
    // "timezones": [ "Europe/Kyiv", "America/New_York" ],
    "actions": {
      "on-click-right": "mode"
    },
    "calendar": {
      "mode": "month",
      "mode-mon-col": 3,
      "weeks-pos": "right",
      "on-scroll": 1,
      "on-click-right": "mode",
      "format": {
        "months": "<span color='#f4dbd6'><b>{}</b></span>",
        "days": "<span color='#cad3f5'><b>{}</b></span>",
        "weeks": "<span color='#c6a0f6'><b>W{}</b></span>",
        "weekdays": "<span color='#a6da95'><b>{}</b></span>",
        "today": "<span color='#8bd5ca'><b><u>{}</u></b></span>"
      }
    }
  },
  "custom/media": {
    "format": "{icon}Û∞éà",
    "restart-interval": 2,
    "return-type": "json",
    "format-icons": {
      "Playing": "ÔÖÑ",
      "Paused": "Ôäã"
    },
    "max-length": 35,
    "exec": "fish -c fetch_music_player_data",
    "on-click": "playerctl play-pause",
    "on-click-right": "playerctl next",
    "on-click-middle": "playerctl prev",
    "on-scroll-up": "playerctl volume 0.05-",
    "on-scroll-down": "playerctl volume 0.05+",
    "smooth-scrolling-threshold": "0.1"
  },
  "bluetooth": {
    "format": "Û∞ÇØ",
    "format-disabled": "Û∞Ç≤",
    "format-connected": "Û∞Ç± {device_alias}",
    "format-connected-battery": "Û∞Ç± {device_alias} (Û∞•â {device_battery_percentage}%)",
    // "format-device-preference": [ "device1", "device2" ], // preference list deciding the displayed device
    "tooltip-format": "{controller_alias}\t{controller_address} ({status})\n\n{num_connections} connected",
    "tooltip-format-disabled": "bluetooth off",
    "tooltip-format-connected": "{controller_alias}\t{controller_address} ({status})\n\n{num_connections} connected\n\n{device_enumerate}",
    "tooltip-format-enumerate-connected": "{device_alias}\t{device_address}",
    "tooltip-format-enumerate-connected-battery": "{device_alias}\t{device_address}\t({device_battery_percentage}%)",
    "max-length": 35,
    "on-click": "fish -c bluetooth_toggle",
    "on-click-right": "overskride"
  },
  "network": {
    "format": "Û∞§≠",
    "format-wifi": "{icon}({signalStrength}%){essid}",
    "format-icons": [
      "Û∞§Ø",
      "Û∞§ü",
      "Û∞§¢",
      "Û∞§•",
      "Û∞§®"
    ],
    "format-disconnected": "Û∞§´ Disconnected",
    "tooltip-format": "wifi <span color='#ee99a0'>off</span>",
    "tooltip-format-wifi": "SSID: {essid}({signalStrength}%), {frequency} MHz\nInterface: {ifname}\nIP: {ipaddr}\nGW: {gwaddr}\n\n<span color='#a6da95'>Ó™°{bandwidthUpBits}</span>\t<span color='#ee99a0'>Ó™ö{bandwidthDownBits}</span>\t<span color='#c6a0f6'>Û∞ππ{bandwidthTotalBits}</span>",
    "tooltip-format-disconnected": "<span color='#ed8796'>disconnected</span>",
    // "format-ethernet": "Û∞àÄ {ipaddr}/{cidr}",
    // "format-linked": "Û∞àÄ {ifname} (No IP)",
    // "tooltip-format-ethernet":"Interface: {ifname}\nIP: {ipaddr}\nGW: {gwaddr}\nNetmask: {netmask}\nCIDR: {cidr}\n\n<span color='#a6da95'>Ó™°{bandwidthUpBits}</span>\t<span color='#ee99a0'>Ó™ö{bandwidthDownBits}</span>\t<span color='#c6a0f6'>Û∞ππ{bandwidthTotalBits}</span>",
    "max-length": 35,
    "on-click": "fish -c wifi_toggle",
    "on-click-right": "iwgtk"
  },
  "group/misc": {
    "orientation": "horizontal",
    "modules": [
      // "custom/webcam",
      "privacy",
      "custom/recording",
      // "custom/geo",
      "custom/media",
      "custom/dunst",
      "idle_inhibitor"
    ]
  },
  "custom/webcam": {
    "interval": 1,
    "exec": "fish -c check_webcam",
    "return-type": "json"
  },
  "privacy": {
    "icon-spacing": 1,
    "icon-size": 12,
    "transition-duration": 250,
    "modules": [
      {
        "type": "audio-in"
      },
      {
        "type": "screenshare"
      }
    ]
  },
  "custom/recording": {
    "interval": 1,
    "exec-if": "pgrep wl-screenrec",
    "exec": "fish -c check_recording",
    "return-type": "json"
  },
  "custom/geo": {
    "interval": 1,
    "exec-if": "pgrep geoclue",
    "exec": "fish -c check_geo_module",
    "return-type": "json"
  },
  "custom/airplane_mode": {
    "return-type": "json",
    "interval": 1,
    "exec": "fish -c check_airplane_mode",
    "on-click": "fish -c airplane_mode_toggle"
  },
  "custom/night_mode": {
    "return-type": "json",
    "interval": 1,
    "exec": "fish -c check_night_mode",
    "on-click": "fish -c night_mode_toggle"
  },
  "custom/dunst": {
    "return-type": "json",
    "exec": "fish -c dunst_pause",
    "on-click": "dunstctl set-paused toggle",
    "restart-interval": 1
  },
  "idle_inhibitor": {
    "format": "{icon}",
    "format-icons": {
      "activated": "Û∞õê",
      "deactivated": "Û∞õë"
    },
    "tooltip-format-activated": "idle-inhibitor <span color='#a6da95'>on</span>",
    "tooltip-format-deactivated": "idle-inhibitor <span color='#ee99a0'>off</span>",
    "start-activated": true
  },
  "custom/logout_menu": {
    "return-type": "json",
    "exec": "echo '{ \"text\":\"Û∞ê•\", \"tooltip\": \"logout menu\" }'",
    "interval": "once",
    "on-click": "fish -c wlogout_uniqe"
  },
  "cpu": {
    "format": "Û∞ª† {usage}%",
    "states": {
      "high": 90,
      "upper-medium": 70,
      "medium": 50,
      "lower-medium": 30,
      "low": 10
    },
    "on-click": "wezterm start btop",
    "on-click-right": "wezterm start btm"
  },
  "memory": {
    "format": "Ôãõ {percentage}%",
    "tooltip-format": "Main: ({used} GiB/{total} GiB)({percentage}%), available {avail} GiB\nSwap: ({swapUsed} GiB/{swapTotal} GiB)({swapPercentage}%), available {swapAvail} GiB",
    "states": {
      "high": 90,
      "upper-medium": 70,
      "medium": 50,
      "lower-medium": 30,
      "low": 10
    },
    "on-click": "wezterm start btop",
    "on-click-right": "wezterm start btm"
  },
  "temperature": {
    "tooltip": false,
    "thermal-zone": 8,
    "critical-threshold": 80,
    "format": "{icon}{temperatureC}Û∞îÑ",
    "format-critical": "üî•{icon}{temperatureC}Û∞îÑ",
    "format-icons": [
      "Ôãã",
      "Ôãä",
      "Ôãâ",
      "Ôãà",
      "Ôãá"
    ]
  },
  "pulseaudio": {
    "states": {
      "high": 90,
      "upper-medium": 70,
      "medium": 50,
      "lower-medium": 30,
      "low": 10
    },
    "tooltip-format": "{desc}",
    "format": "{icon}{volume}% {format_source}",
    "format-bluetooth": "Û∞Ç±{icon}{volume}%\n{format_source}",
    "format-bluetooth-muted": "Û∞Ç±Û∞ùü{volume}%\n{format_source}",
    "format-muted": "Û∞ùü{volume}%\n{format_source}",
    "format-source": "Û∞ç¨{volume}%",
    "format-source-muted": "Û∞ç≠{volume}%",
    "format-icons": {
      "headphone": "Û∞ãã",
      "hands-free": "ÓåΩ",
      "headset": "Û∞ãé",
      "phone": "Û∞Ñú",
      "portable": "Û∞¶ß",
      "car": "Û∞Ñã",
      "speaker": "Û∞ìÉ",
      "hdmi": "Û∞°Å",
      "hifi": "Û∞ãå",
      "default": [
        "Û∞ïø",
        "Û∞ñÄ",
        "Û∞ïæ"
      ]
    },
    "reverse-scrolling": true,
    "reverse-mouse-scrolling": true,
    "on-click": "pavucontrol"
  },
  "systemd-failed-units": {
    "format": "‚úó {nr_failed}"
  }
}
